/* M0054 - J6 - Camera Group 0 - GPIO 28-31 */
&qupv3_se0_spi {
	status = "ok";
	spidev@0 {
		compatible = "spidev";
		reg = <0>;
		spi-max-frequency = <50000000>;
	};
};

